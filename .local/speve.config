# Speve Discord Bot Configuration
# This file contains optional configuration settings for the bot.
# Format: KEY=VALUE (one per line)
# Lines starting with # are comments and will be ignored.

# Bot command prefix (default: !)
COMMAND_PREFIX=!

# Logging level: DEBUG, INFO, WARNING, ERROR, CRITICAL (default: INFO)
LOG_LEVEL=INFO

# Enable voice functionality: true or false (default: true)
VOICE_ENABLED=true

# Text-to-speech language code (default: en)
# Common codes: en (English), es (Spanish), fr (French), de (German), etc.
TTS_LANGUAGE=en

# Text-to-speech speed: true for slow, false for normal (default: false)
TTS_SLOW=false

# Additional Discord.py settings (uncomment to use)
# MAX_MESSAGES=1000
# HEARTBEAT_TIMEOUT=60